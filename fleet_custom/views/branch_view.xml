<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.ui.view" id="company_branch_form_view">
        <field name="name">company.branch.form.inherit</field>
        <field name="model">company.branch</field>
        <field name="inherit_id" ref="aspl_company_branch_ee.company_branch_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="code"/>
                <field name="map_link"/>
            </xpath>
        </field>
    </record>
    <record model="ir.ui.view" id="company_branch_tree_view">
        <field name="name">company.branch.tree.inherit</field>
        <field name="model">company.branch</field>
        <field name="inherit_id" ref="aspl_company_branch_ee.company_branch_tree_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='company_id']" position="after">
                <field name="code"/>
            </xpath>
        </field>
    </record>
    

</odoo>
